@using Microsoft.AspNet.Identity
@using Pendaftaran_Tenant.Models
@if (@Session["user"] != null)
{
    var u = Session["user"] as Penyewa;
    using (Html.BeginForm("Logout", "Penyewa", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
        @Html.AntiForgeryToken()

        <ul class="nav navbar-nav navbar-right">
            <li>
                @Html.ActionLink("Hello " + u.email + "!", "Index", "Manage", routeValues: null, htmlAttributes: new { title = "Manage" })
            </li>
            <li><a href="javascript:document.getElementById('logoutForm').submit()">Log off</a></li>
        </ul>
    }
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Daftar", "Create", "Penyewa", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>
        <li>@Html.ActionLink("Log in", "Login", "Penyewa", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    </ul>
}
