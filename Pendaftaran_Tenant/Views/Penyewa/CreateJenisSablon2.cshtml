@{ var namaproduk = Session["nama_produk"];}


<h2>Isi Bahan untuk produk @namaproduk</h2>
@Html.Raw(TempData["Pesan"])
<div class="row">
    <div class="col-md-8">

        @using (Html.BeginForm("CreateJenisSablon2", "Penyewa", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
        {
            @Html.AntiForgeryToken()

            <hr />

            <div class="form-group">
                <label for="namajenissablon" class="col-md-2 control-label">Nama Jenis Sablon</label>
                <div class="col-md-10">
                    <input type="text" class="form-control" id="namajenissablon" name="namajenissablon" />

                </div>
            </div>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Simpan" class="btn btn-default" />
                </div>
            </div>

        }

    </div>

</div>



