@model Pendaftaran_Tenant.Models.Harga

@{
    ViewBag.Title = "EditHarga";
}

<br />
<div class="col-md-12 col-xs-12" style="margin-left:32%">
    <div class="thumbnail clearfix text-center" style="max-width:400px;width:100%">

        @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()


        <h4>Edit Harga</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <table style="border:0 solid;float:none;margin:0 auto">
            <tr>
                <th style="text-align:center"> Harga baru</th>
            </tr>
            <tr>
                <td>
                    @Html.EditorFor(model => model.harga, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.harga, "", new { @class = "text-danger" })
                </td>
            </tr>
        </table>
        <br />
        <button class="btn btn-warning" onclick="location.href='javascript:history.go(-1)'">Batal</button>
        <input type="submit" value="Save" class="btn btn-default" />
        <br />
        <br />

}

    </div>
</div>